{
  "name": "p-vui",
  "version": "1.3.1",
  "description": "A Private Component Library for Vue.js .",
  "main": "lib/vui.js",
  "style": "lib/vui-css/index.css",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "private": false,
  "scripts": {
    "bootstrap": "cnpm install",
    "dev": "vue-cli-service serve",
    "deploy": "npm run deploy:docs && gh-pages -d examples/dist",
    "deploy:docs": "rimraf examples/dist && node build/build.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dist": "npm run clean && npm run lint && npm run build:vui && npm run build:vui-css",
    "release": "npm run bootstrap && sh build/release.sh",
    "start": "npm run dev",
    "unit": "vue-cli-service test:unit",
    "test": "npm run unit",
    "test:coverage": "find test/unit/coverage/lcov-report -name 'index.html' | sed -n 1,1p | xargs -I {} open {} ",
    "lint": "vue-cli-service lint",
    "build:vui": "vue-cli-service build",
    "build:vui-css": "gulp build --gulpfile packages/vui-css/gulpfile.js && rimraf lib/vui-css && cp-cli packages/vui-css/lib lib/vui-css"
  },
  "keywords": [
    "vui",
    "vue",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brickies/vui.git"
  },
  "author": "qiangdada",
  "license": "MIT",
  "homepage": "https://github.com/Brickies/vui.git",
  "bugs": {
    "url": "https://github.com/Brickies/vui/issues"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-unit-mocha": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "avoriaz": "^6.3.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "child_process": "^1.0.2",
    "cp-cli": "^1.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-vue-md-loader": "^1.0.3",
    "file-save": "^0.2.0",
    "gh-pages": "^2.0.1",
    "inject-loader": "^4.0.1",
    "inquirer": "^6.2.1",
    "karma": "^3.1.4",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^7.2.2",
    "node-notifier": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-salad": "^2.0.1",
    "postcss-url": "^8.0.0",
    "precss": "^4.0.0",
    "saladcss-bem": "0.0.2",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "uppercamelcase": "^3.0.0",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">= 6.5.0"
  }
}
